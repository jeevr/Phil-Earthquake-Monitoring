				"type": "String"
			},
			{
				"name": "longitude",
				"type": "String"
			},
			{
				"name": "depth_km",
				"type": "String"
			},
			{
				"name": "depth_km_symbol",
				"type": "String"
			},
			{
				"name": "magnitude",
				"type": "String"
			},
			{
				"name": "location",
				"type": "String"
			},
			{
				"name": "hlink",
				"type": "String"
			},
			{
				"name": "details",
				"type": "String"
			}
		]
